    "express": "^4.18.2",
    "express-session": "^1.19.0",
    "gh-pages": "^6.0.0",
    "jsonwebtoken": "^9.0.3",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
